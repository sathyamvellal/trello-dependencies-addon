{
  "manifest_version": 2,

  "name": "Trello Dependency Addon",
  "description": "Trello extension to intuitively manage and view dependencies",
  "version": "0.0.1",

  "permissions": [
    "*://trello.com/*"
  ],

  "content_scripts": [
    {
      "matches": ["*://trello.com/b/*"],
      "run_at": "document_end",
      "js": ["jquery-1.11.3.min.js", "board.js"]
    },
    {
      "matches": ["*://trello.com/c/*"],
      "run_at": "document_end",
      "js": ["jquery-1.11.3.min.js", "card.js"]
    }
  ]
}
